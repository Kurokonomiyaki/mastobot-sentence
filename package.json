{
  "name": "mastobot-sentence",
  "version": "0.0.1",
  "description": "A simple sentence generator for Mastodon",
  "main": "compiled/index.js",
  "scripts": {
    "compile": "node ./node_modules/babel-cli/bin/babel.js src --out-dir compiled",
    "token": "node ./node_modules/mastodon-api-token/index-compiled.js",
    "execute": "babel-node src/index.js"
  },
  "author": "Kuro",
  "license": "",
  "dependencies": {
    "babel-runtime": "6.26.0",
    "mastodon-api": "1.3.0",
    "mastodon-api-token": "git+https://github.com/kurokonomiyaki/mastodon-api-token.git",
    "merge-options": "1.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "eslint": "5.13.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4"
  }
}
